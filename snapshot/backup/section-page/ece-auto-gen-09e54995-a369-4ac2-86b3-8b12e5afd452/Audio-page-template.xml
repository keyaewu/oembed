<?xml version="1.0" encoding="UTF-8"?>
<escenic version="2.0" xmlns="http://xmlns.escenic.com/2009/import">
    <section-page source="ece-auto-gen" sourceid="09e54995-a369-4ac2-86b3-8b12e5afd452" unique-name="config.oembed.article.type.audioIngest" name="Audio page template" layout-name="oembed-config" action="remove">
        <area name="oembed-config-area">
            <options>
                <field name="htmltag">
                    <value>div</value>
                </field>
            </options>
            <group name="contextSectionGroup" label="Context Section">
                <options>
                    <field name="reference">
                        <value>menu</value>
                    </field>
                    <field name="referable">false</field>
                </options>
            </group>
            <group name="wrapperGroup" label="Wrapper">
                <area name="wrapperGroup-area">
                    <options>
                        <field name="loadingpolicy">
                            <value>onPage</value>
                        </field>
                    </options>
                </area>
                <options>
                    <field name="htmltag">
                        <value>div</value>
                    </field>
                    <field name="referable">false</field>
                </options>
            </group>
            <group name="navbar" label="Navbar">
                <area name="nav">
                    <options>
                        <field name="loadingpolicy">
                            <value>onPage</value>
                        </field>
                    </options>
                </area>
                <options>
                    <field name="htmltag">
                        <value>nav</value>
                    </field>
                    <field name="referable">false</field>
                    <field name="navbarstyle">
                        <value>navbar-default</value>
                    </field>
                    <field name="navbarpostion">
                        <value>no-class</value>
                    </field>
                </options>
            </group>
            <group name="carouselGroup" label="Carousel">
                <options>
                    <field name="htmltag">
                        <value>div</value>
                    </field>
                    <field name="referable">false</field>
                    <field name="heightoption">
                        <value>auto</value>
                    </field>
                    <field name="shownavigation">true</field>
                    <field name="showpreviousnext">true</field>
                    <field name="autoplay">false</field>
                    <field name="autoplayinterval">5.00</field>
                    <field name="height">100.00</field>
                </options>
            </group>
            <group name="tabbingGroup" label="Tabs">
                <options>
                    <field name="changeurl">true</field>
                    <field name="htmltag">
                        <value>div</value>
                    </field>
                    <field name="referable">false</field>
                    <field name="tabstyle">
                        <value>tabs</value>
                    </field>
                </options>
            </group>
            <group name="split-3" label="Three column">
                <area name="column-1">
                    <options>
                        <field name="loadingpolicy">
                            <value>onPage</value>
                        </field>
                        <field name="htmltag">
                            <value>div</value>
                        </field>
                        <field name="column-1-span">
                            <value>inherited</value>
                        </field>
                    </options>
                </area>
                <area name="column-2">
                    <options>
                        <field name="column-2-span">
                            <value>inherited</value>
                        </field>
                        <field name="loadingpolicy">
                            <value>onPage</value>
                        </field>
                        <field name="htmltag">
                            <value>div</value>
                        </field>
                    </options>
                </area>
                <area name="column-3">
                    <options>
                        <field name="column-3-span">
                            <value>inherited</value>
                        </field>
                        <field name="loadingpolicy">
                            <value>onPage</value>
                        </field>
                        <field name="htmltag">
                            <value>div</value>
                        </field>
                    </options>
                </area>
                <options>
                    <field name="htmltag">
                        <value>div</value>
                    </field>
                    <field name="split">
                        <value>4_4_4</value>
                    </field>
                    <field name="referable">false</field>
                </options>
            </group>
            <group name="row" label="Row">
                <options>
                    <field name="htmltag">
                        <value>div</value>
                    </field>
                    <field name="referable">false</field>
                </options>
            </group>
            <group name="wrapperGroup" label="Wrapper">
                <area name="wrapperGroup-area">
                    <options>
                        <field name="loadingpolicy">
                            <value>onPage</value>
                        </field>
                    </options>
                    <group name="split-2" label="Two column">
                        <area name="column-1">
                            <options>
                                <field name="loadingpolicy">
                                    <value>onPage</value>
                                </field>
                                <field name="htmltag">
                                    <value>div</value>
                                </field>
                                <field name="column-1-span">
                                    <value>inherited</value>
                                </field>
                            </options>
                        </area>
                        <area name="column-2">
                            <options>
                                <field name="column-2-span">
                                    <value>inherited</value>
                                </field>
                                <field name="loadingpolicy">
                                    <value>onPage</value>
                                </field>
                                <field name="htmltag">
                                    <value>div</value>
                                </field>
                            </options>
                            <content-ref source="ece-auto-gen" sourceid="2a60b34f-0954-4705-855c-c4e3913836dc">
                                <field name="title">video player</field>
                                <options>
                                    <field name="loadingpolicy">
                                        <value>onPage</value>
                                    </field>
                                </options>
                            </content-ref>
                        </area>
                        <options>
                            <field name="loadingpolicy">
                                <value>onPage</value>
                            </field>
                            <field name="htmltag">
                                <value>div</value>
                            </field>
                            <field name="split">
                                <value>6_6</value>
                            </field>
                            <field name="referable">false</field>
                        </options>
                    </group>
                </area>
                <options>
                    <field name="com.escenic.color">#ccccff</field>
                    <field name="htmltag">
                        <value>div</value>
                    </field>
                    <field name="referable">true</field>
                </options>
            </group>
        </area>
    </section-page>
</escenic>
